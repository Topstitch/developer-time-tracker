<!DOCTYPE html>
<html>
<head>
  <title>DeveloperTimeTracker</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<p id="notice"><%= notice %></p>


<% if session[:developer_id] %>
  <p><%= link_to 'Log Out', login_path(session[:developer_id]), method: :delete, data: { confirm: 'Are you sure?' } %></p>

<% end %>

<%= yield %>

</body>
</html>
