<%= form_tag logins_path do %>
  <div class = "form-group">
    <%= label_tag :email %><br />
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class = "form-group">
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </div>
  <div class = "form-group"><%= submit_tag "Log in", class: "btn btn-default" %></div>
<% end %>
